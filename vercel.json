{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "Flynest/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
